{
  "name": "unified-ai-frontend",
  "version": "1.0.0",
  "description": "Clean React frontend for Unified AI Engine - Workflow debugging and automation analysis UI",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "dev:local": "APP_ENV=local next dev -p 3001",
    "dev:development": "APP_ENV=development next dev -p 3001",
    "build": "next build",
    "build:local": "APP_ENV=local next build",
    "build:development": "APP_ENV=development next build",
    "build:staging": "APP_ENV=staging next build",
    "build:production": "APP_ENV=production next build",
    "start": "next start -p 3001",
    "start:local": "APP_ENV=local next start -p 3001",
    "start:development": "APP_ENV=development next start -p 3001",
    "start:staging": "APP_ENV=staging next start -p 3001",
    "start:production": "APP_ENV=production next start -p 3001",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "health": "node health-check.js",
    "env:validate": "node scripts/validate-env.js",
    "deploy:staging": "npm run build:staging && npm run deploy:k8s:staging",
    "deploy:production": "npm run build:production && npm run deploy:k8s:production"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "@types/react-syntax-highlighter": "^15.5.0",
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^10.16.0",
    "lucide-react": "^0.290.0",
    "next": "^14.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.0",
    "react-syntax-highlighter": "^15.5.0",
    "recharts": "^3.3.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.51.0",
    "eslint-config-next": "^14.0.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.2.0"
  }
}
